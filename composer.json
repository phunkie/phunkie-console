{
  "name": "phunkie/phunkie-console",
  "description": "A console for Phunkie development",
  "license":      "MIT",
  "authors": [
    {
      "name": "Marcello Duarte",
      "email": "marello.duarte@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "phunkie/phunkie": "^0.11.5",
    "nikic/php-parser": "^5.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^7.0",
    "friends-of-phpspec/phpspec-expect": "^4.0",
    "behat/behat": "^3.12"
  },
  "autoload": {
    "psr-4": {
      "PhunkieConsole\\": [ "src/"]
    },
    "files": [
      "src/.__init.php"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "bin": ["bin/phunkie"]
}
